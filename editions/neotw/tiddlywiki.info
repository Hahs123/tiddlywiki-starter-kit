{
  "description": "neotw empty edition",
  "languages": [
    "zh-Hans"
  ],
  "plugins": [
    "tiddlywiki/tiddlyweb",
    "tiddlywiki/filesystem",
    "tiddlywiki/highlight",
    "tiddlywiki/katex",
    "tiddlywiki/browser-sniff",
    "tiddlywiki/codemirror",
    "tiddlywiki/codemirror-autocomplete",
    "tiddlywiki/codemirror-mode-css",
    "tiddlywiki/codemirror-search-replace",
    "tiddlywiki/codemirror-closebrackets",
    "tiddlywiki/codemirror-mode-markdown",
    "tiddlywiki/codemirror-mode-xml",
    "tiddlywiki/codemirror-mode-javascript",
    "oeyoews/markdown",
    "oeyoews/neotw",
    "oeyoews/neotw-confetti",
    "oeyoews/neotw-hitokoto",
    "oeyoews/neotw-music",
    "oeyoews/neotw-vimjk",
    "oeyoews/NPL",
    "oeyoews/tiddlywiki-announcement",
    "oeyoews/tiddlywiki-back-to-top",
    "oeyoews/tiddlywiki-edition-macro",
    "oeyoews/tiddlywiki-fancybox-ui",
    "oeyoews/tiddlywiki-giscus",
    "oeyoews/tiddlywiki-github-share",
    "oeyoews/tiddlywiki-keyboard-hint",
    "oeyoews/tiddlywiki-mobile-warning",
    "oeyoews/tiddlywiki-modal-ui",
    "oeyoews/tiddlywiki-motion",
    "oeyoews/tiddlywiki-publish-tiddler",
    "oeyoews/tiddlywiki-static-page",
    "oeyoews/tiddlywiki-tailwindcss",
    "oeyoews/tiddlywiki-tiddler-info",
    "oeyoews/tiddlywiki-widgets"
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "nico/notebook"
  ],
  "build": {
    "listen": [
      "--listen",
      "port=8080"
    ],
    "index": [
      "--verbose",
      "--render",
      "$:/plugins/tiddlywiki/tiddlyweb/save/offline",
      "index.html",
      "text/plain",
      "--savetiddler",
      "$:/favicon.ico",
      "favicon.ico",
      "--deletetiddlers",
      "$:/StoryList"
    ]
  }
}
