{
  "apps": [
    {
      "name": "neotw",
      "script": "./node_modules/tiddlywiki/tiddlywiki.js",
      "cwd": "./",
      "watch": [
        "./dev/plugins/",
        "tiddlers"
      ],
      "watch_delay": 1000,
      "args": "--build listen",
      "instances": 1,
      "error_file": "./logs/error.log",
      "out_file": "./logs/out.log",
      "log_date_format": "YYYY-MM-DD HH:mm Z"
    }
  ]
}
