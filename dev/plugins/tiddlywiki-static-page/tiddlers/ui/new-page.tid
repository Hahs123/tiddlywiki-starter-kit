title: $:/plugins/oeyoews/tiddlywiki-static-page/ui/new-page
tags: $:/tags/ViewToolbar
description: new-page
caption: new-page
list-before: $:/core/ui/Buttons/close

\whitespace trim

<$list filter="[all[current]!is[system]]">
<$button  tooltip="jump new-page and copy url" aria-label="new-page" class=<<tv-config-toolbar-class>>>
<$list filter="[{$:/info/url/protocol}prefix[https]]">
<!-- https -->
<$action-sendmessage $message="tm-copy-to-clipboard" $param={{{ [[$:/info/url/full]get[text]search-replace:g:regexp[index.html|index|main.html|main|],[]addsuffix[static/]] [[]addsuffix<storyTiddler>encodeuricomponent[]] +[join[]] }}}/>
<$action-sendmessage $message="tm-open-external-window" $param={{{ [[$:/info/url/full]get[text]search-replace:g:regexp[index.html|index|main.html|main|],[]addsuffix[static/]] [[]addsuffix<storyTiddler>encodeuricomponent[]] +[join[]] }}}/>
</$list>
<$list filter="[{$:/info/url/protocol}prefix[http:]]">
<!-- http  -->
<$action-sendmessage $message="tm-copy-to-clipboard" $param={{{ [[$:/info/url/full]get[text]] [[]addsuffix<storyTiddler>encodeuricomponent[]] +[join[]] }}}/>
<$action-sendmessage $message="tm-open-external-window" $param={{{ [[$:/info/url/full]get[text]] [[]addsuffix<storyTiddler>encodeuricomponent[]] +[join[]] }}}/>
</$list>
<$list filter="[<tv-config-toolbar-icons>match[yes]]">
{{$:/plugins/oeyoews/tiddlywiki-static-page/icon}}
</$list>
<$list filter="[<tv-config-toolbar-text>match[yes]]">
<span class="tc-btn-text">
<$text text="new-page"/>
</span>
</$list>
</$button>
</$list>
