title: $:/plugins/oeyoews/neotw/modules/darkmode/actions
tags: $:/tags/StartupAction

<$list filter="[{$:/info/darkmode}match[yes]]">
<$action-setfield $tiddler="$:/palette" text="$:/palettes/GithubDark"/>
</$list>

<$list filter="[{$:/info/darkmode}match[no]]">
<$action-setfield $tiddler="$:/palette" text="$:/themes/nico/notebook/palettes/palette-beige"/>
</$list>
