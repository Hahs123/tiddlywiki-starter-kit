title: $:/plugins/oeyoews/neotw/modules/landing/style
tags: $:/tags/Stylesheet
type: text/css

.o-landing {
	margin: 1em 0.5em;
}

@media (min-width: 760px) {
	.o-landing {
		width: 660px;
		margin: 1em auto;
	}
}

.o-landing-branding {
	text-align: center;
	margin-bottom: 4em;
}

.o-landing-branding h1 {
	font-weight: bold;
}

.landing-button-start {
    padding: 10px;
    font-weight: bold;
  }

.landing-button-start:hover {
  transform: scale(0.95);
  background-color: aliceblue !important;
  transition: transform 0.2s;
}
