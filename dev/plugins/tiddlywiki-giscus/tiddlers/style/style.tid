title: $:/plugins/oeyoews/tiddlywiki-giscus/style
tags: $:/tags/Stylesheet

<!-- hide powered -->
.gsc-left-header {
    display: none !important;
}

#__next .text-sm {
    font-size: .875rem;
    line-height: 1.25rem;
		color: red !important;
}

.get-started {
		display: inline-block;
    font-size: 16px;
    background-color: rgb(97, 218, 251);
    color: rgb(0, 0, 0);
    padding: 10px 25px;
    border-radius: 5px;
    border-color: white;
    white-space: nowrap;
    transition: background-color 0.2s ease-out 0s;
}

.nbtn {
		display: inline-block;
    background-color: rgb(97, 218, 251);
    color: rgb(0, 0, 0);
    padding: 3px 8px;
    white-space: nowrap;
    border-radius: 5px;
    transition: background-color 0.2s ease-out 0s;
}
