.gk0wk-caticon img { fill: #0A84FF; }

.gk0wk-rotate-infinite { 
	animation: move 15s ease-in infinite; 
}

@keyframes move
{
	from {left:0px;}
	to {
	left: 400px;
	}
}

@keyframes rotate{ 
	from {
		transform: rotate(0deg)
	}
	to {
		transform: rotate(360deg)
	} 
}