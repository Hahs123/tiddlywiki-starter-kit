<$list filter="[{$:/info/url/search}prefix[?filter=temp]]">
  <$action-setfield $tiddler="$:/DefaultTiddlers" text="[field:temp[yes]!sort[modified]]"/>
</$list>

<$list filter="[{$:/info/url/search}prefix[?filter=oeyoews]]">
